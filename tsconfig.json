{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "allowJs": true,
    "strict": true,
    "types": [],
    "lib": [
      "dom",
      "es6",
      "es2017"
    ],
    "baseUrl": ".",
    "target": "es5",
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".cache",
    "src/worker.js",
    "heartbeat"
  ]
}